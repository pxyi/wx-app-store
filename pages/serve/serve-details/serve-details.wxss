page{
  background: #F0F0F0;
}

.apply-info,
.store-info{
  padding: 0 35rpx;
  margin: 20rpx 0;
  background: #fff;
}
.apply-title{
  padding: 40rpx 0 20rpx;
  margin-bottom: 30rpx;
  font-size: 32rpx;
  border-bottom: solid 1px #f0f0f0;
  overflow: hidden;
  position: relative;
}
.apply-title view:first-child{
  float: left;
}
.apply-title view:last-child{
  float: right;
}
.apply-cell{
  padding-bottom: 16rpx;
}
.apply-cell-desc{
  color: #63686e;
  font-size: 26rpx;
  line-height: 26rpx;
  padding-bottom: 24rpx;
}

.store-info{
  padding-bottom: 26rpx;
  overflow: hidden;
}
.title-button{
  width: 80rpx;
  color: #FF839A;
  font-size: 22rpx;
  line-height: 33rpx;
  text-align: center;

  position: absolute;
  top: 50%;
  right: 0;
  z-index: 9;
  margin-top: -10rpx;
  transition: all .2s;
}
.title-button.active{
  color: #fff;
  background: #F38A03;
}
.store-item{
  padding-bottom: 8rpx;
  color: #63686e;
  font-size: 26rpx;
  line-height: 40rpx;
}


.footer{
  width: 100%;
  text-align: center;
  padding:80rpx 0 50rpx 0;
}
.footer-phone{
   color: rgb(150, 150, 150);
  font-size: 26rpx;
  line-height: 36rpx;

}
.footer-time{
  color: rgb(150, 150, 150);
  font-size: 23rpx;
  line-height: 23rpx;
  padding-top: 15rpx;
}

.fixed-btns{
  width: 100%;
  position: fixed;
  bottom: 0;
  height: 100rpx;

}
.delete-btn{
  width: 200rpx;
  color: #63686e;
  font-size: 26rpx;
  line-height: 100rpx;
  text-align: center;
  background: #fff;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 9;
}
.delete-btn.active{
  background: #f5f5f5;
}

.reset-btn{
  padding-left: 200rpx;
  color: #fff;
  font-size: 34rpx;
  line-height: 100rpx;
  text-align: center;
  background: #FF839A;
  box-sizing: border-box;
}
.reset-btn.active{
  opacity: .8;
}
.serve_tit{
  width: 690rpx;
  margin: 0 auto;
  padding:0 30rpx;
  background: #fff;
  height: 100rpx;
  line-height: 100rpx;
  margin-top: 20rpx;
  font-size: 32rpx;
}
.serve_tit text{
  float: right;
}
.store_left{
  width: 120rpx;
  height: 120rpx;
  float: left;
  margin-right: 24rpx;
  margin-top: 8rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.store_left image{
  width: 100%;
  height: 100%;
}
.store_right{
  width: 500rpx;
  float: left;
}

.ticket{
  width: 690rpx;
  height: 100rpx;
  background: url('https://ylbb-wxapp.oss-cn-beijing.aliyuncs.com/store/server_banner4554.png')no-repeat;
  background-size:100% 100%;
  margin-top: 30rpx; 
  overflow: hidden;
}
.ticket_banner{
  height: 40rpx;
  color: #FFF;
  font-size: 30rpx;
  overflow: hidden;
  margin-top: 30rpx;
}
.ticket_banner text:first-child{
  width: 35rpx;
  height: 35rpx;
  line-height: 35rpx;
  font-size: 22rpx;
  background:#FD5576;
  display: inline-block;
  text-align: center;
  border-radius: 10rpx;
  margin: 2.5rpx 30rpx 0 30rpx;
}
.ticket_banner text:last-child{
  padding-left: 20rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  color: #FD5576;
  display: inline-block;
  border-left: solid 1rpx #FD5576;
}
.ticket_list{
  width: 100%;
  overflow: hidden;
  margin-top: 35rpx;
}
.ticket_left{
  width: 500rpx;
  font-size: 26rpx;
  color: #aaa;
  float: left;
}
.ticket_left view{
  height: 50rpx;
  line-height: 50rpx;
}
.ticke_right{
  width: 114rpx;
  font-size: 22rpx;
  color: #aaa;
  height: 44rpx;
  line-height: 44rpx;
  padding: 0;
  margin: 0;
  float: right;
  border:solid 1rpx #aaa;
  text-align: center;
  border-radius: 10rpx;
  background: none;
  box-shadow: 0;
}
.store-info-content1 view{
  font-size: 26rpx;
  color: #aaa;
  margin-top: 20rpx;
}
.tkSuccess{
  background: #fff;
  padding: 50rpx 0;
  margin-top: 20rpx;
}
.tkSuccess_icon{
  width: 80rpx;
  height: 80rpx;
  margin: 0 auto;
  margin-top: 20rpx;
}
.tkSuccess_icon image{
  width: 100%;
  height: 100%;
}
.tkSuccess_text{
  width:100%;
  color: #aaa;
  font-size: 24rpx;
  text-align: center;
  margin-top: 20rpx;
}
.tkSuccess_text text{
  font-size: 32rpx;
  color: #000;
  display: block;
  height: 50rpx;
  line-height: 50rpx;
  font-weight: 600;
}
.ticket_left{
  font-size: 24rpx;
}