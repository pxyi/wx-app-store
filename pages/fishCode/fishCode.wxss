page{
  background: #F2F2F2;
}
.content{
  width: 100%;
  overflow: hidden;
  background: #fff;
}
.title{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 36rpx;
  color: #4A4A4A;
  text-align: center;
  margin-top: 100rpx;
}
.header input{
  display: block;
  width: 570rpx;
  height:92rpx;
  padding: 0 30rpx;
  border: solid 2rpx #DBDBDB;
  border-radius: 5px;
  margin: 0 auto;
  font-size: 32rpx;
  margin-top: 24rpx;
}
.header button{
  width: 630rpx;
  height: 92rpx;
  margin-top: 60rpx;
  color: #fff;
  background: #ff1f52;
  margin-bottom: 100rpx;
}
.buttons{
    color: #fff;
  background: #ff1f52;
}
.main{
  width: 630rpx;
  margin: 0 auto;
  font-size: 28rpx;
  color:#9B9B9B;
  margin-top: 50rpx;
}
.main view{
    height: 60rpx;
    line-height: 60rpx;
}
