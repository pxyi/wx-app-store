.item{
  margin: 35rpx 30rpx 0;
  position: relative;
}

.item-cell{
  height: 217rpx;
  padding: 0 8rpx;
  margin-right: 215rpx;
  box-sizing: border-box;
  position: relative;
}
.item-cell::before,
.item-cell::after{
  content: '';
  display: block;
  width: 35rpx;
  height: 217rpx;
  position: absolute;
  top: 0;
}


.item-box{
  padding: 0 27rpx;
  height: 217rpx;

}
.item-box > view{
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}
