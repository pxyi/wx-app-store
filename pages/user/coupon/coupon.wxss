.info_main{
   overflow: hidden;
   position: relative;
   color: #AAAAAA;
}
.useFlag{
  width: 130rpx;
  height: 130rpx;
  text-align: center;
  position: absolute;
  top: 30rpx;
  right: 10rpx;
  border: dashed 5rpx #fff;
  border-radius: 50%;
  font-size: 28rpx;
  color: #fff;
  overflow: hidden;
  transform: rotate(-25deg);
}
.useFlag text{
  display: block;
  height: 50rpx;
  line-height: 50rpx;
  margin-top: 40rpx;
  border: dashed 5rpx #fff;
  border-left: none;
   border-right: none; 
}
.info-h2{
  padding-top: 44rpx;
  line-height: 50rpx;
  font-size: 36rpx;
}
.info-h2 .large{
  font-size: 50rpx;
}
.info-btn{
  width: 148rpx;
  height: 54rpx;
  margin: 25rpx auto 0;
  font-size: 28rpx;
  line-height: 54rpx;
  border: solid 1px #fff;
  border-radius: 6rpx;
  transition: all .15s ease;
}
.item.active .info-btn.active{
  color: #fd7d87;
  background: #fff;
}

.alert-box {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .6);
  position: fixed;
  top: 0;
  z-index: 9;
  opacity: 0;
  visibility: hidden;
  transition: all .2s;
}
.alert-box.active {
  opacity: 1;
  visibility: visible;
}
.alert {
  width: 640rpx;
  background: #fff;
  border-radius: 10rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-top: -300rpx;
  margin-left: -320rpx;
}
.alert-title {
  font-size: 32rpx;
  line-height: 120rpx;
  text-align: center;
}
.alert-content {
  color: #9B9B9B;
  font-size: 30rpx;
  text-align: center;
}
.alert-footer {
  font-size: 32rpx;
  margin-top: 40rpx;
  border-top: solid 1px #F0F0F0;  
}
.alert-footer.active {
  background: rgba(0, 0, 0, .05);
}
.alert-footer-text {
  text-align: center;
  line-height: 96rpx;
  box-sizing: border-box;
}
.alert-footer-text.width {
  width: 50%;
  float: left;
  border-right: solid 1px #F0F0F0;
}
.alert-footer-btn {
  width: 50%;
  float: left;
}
.alert-footer-btn button {
  color: #4caf50;
  background: none;
  font-size: 32rpx;
  line-height: 96rpx;
}
.alert-footer-btn button::after {
  display: none;
}
.alert-cell {
  padding: 0 40rpx;
}
.alert-cell view {
  color: #9B9B9B;
  font-size: 28rpx;
  line-height: 48rpx;
}
